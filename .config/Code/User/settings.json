// Place your settings in this file to overwrite the default settings
{
    "window.zoomLevel": 1,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,

        // Exclude TeX files.
        "**/*.aux": true,
        "**/*.bbl": true,
        "**/*.blg": true,
        "**/*.log": {
            "when": "$(basename).tex"
        }
    }
}