<Multi_key> <bar> <braceleft>	 : "â¦ƒ" U2983	# LEFT WHITE CURLY BRACKET
<Multi_key> <bar> <braceright>	 : "â¦„" U2984	# RIGHT WHITE CURLY BRACKET 
<Multi_key> <bar> <bracketleft>	:   "âŸ¦"	U27E6	# MATHEMATICAL LEFT WHITE SQUARE BRACKET
<Multi_key> <bar> <bracketright>:   "âŸ§"	U27E7	# MATHEMATICAL RIGHT WHITE SQUARE BRACKET
<Multi_key> <2> <less> <parenleft> :   "âŸª"	U27EA	# MATHEMATICAL LEFT DOUBLE ANGLE BRACKET
<Multi_key> <2> <greater> <parenright>   :   "âŸ«"	U27EB	# MATHEMATICAL RIGHT DOUBLE ANGLE BRACKET
<Multi_key> <7> <apostrophe>	    : "ï½¢"     UFF62   # HALFWIDTH LEFT CORNER BRACKET
<Multi_key> <L> <apostrophe>	    : "ï½£"     UFF63   # HALFWIDTH RIGHT CORNER BRACKET
<Multi_key> <7> <quotedbl>	    : "ã€"    U300E   # LEFT WHITE CORNER BRACKET
<Multi_key> <L> <quotedbl>	    : "ã€"    U300F   # RIGHT WHITE CORNER BRACKET

